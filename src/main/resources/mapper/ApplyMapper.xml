    <result column="update_time" property="updateTime" jdbcType="TIMESTAMP" />
  </resultMap>
  
  <select id="queryApplyListWithPage" resultMap="BaseResultMap">
  	select * 
  	from t_apply
  	where 
  		1 = 1
  		<if test="userId != null">
  			and user_id = #{userId}
  		</if>
  		<if test="userName != null  and userName != ''">
  			and user_name LIKE concat('%', #{userName}, '%')
  		</if>
  </select>
</mapper>